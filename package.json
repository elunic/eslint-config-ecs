{
  "name": "@elunic/eslint-config-ecs",
  "version": "0.1.5",
  "description": "The eslint part of the elunic coding styles",
  "main": "index.js",
  "files": [
    "eslintrc/",
    "index.js",
    "fix.js",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "check": "prettier --check './**/*.js'",
    "fix": "prettier --write './**/*.js'",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elunic/eslint-config-ecs.git"
  },
  "keywords": [
    "elunic",
    "eslint",
    "eslintrc",
    "ecs"
  ],
  "author": {
    "name": "William Hefter",
    "email": "wh@elunic.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/eslint": "^7.2.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-ban": "^1.5.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.2.1"
  },
  "peerDependencies": {
    "typescript": ">= 3.0.0 < 5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
