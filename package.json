{
  "name": "@elunic/eslint-config-ecs",
  "version": "8.0.0",
  "description": "The eslint part of the elunic coding styles",
  "main": "index.js",
  "files": [
    "eslintrc/",
    "index.js",
    "fix.js",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "check": "prettier --check './**/*.js'",
    "fix": "prettier --write './**/*.js'",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elunic/eslint-config-ecs.git"
  },
  "keywords": [
    "elunic",
    "eslint",
    "eslintrc",
    "ecs"
  ],
  "author": {
    "name": "William Hefter",
    "email": "wh@elunic.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@elunic/ecs-prettier": "^0.2.2",
    "@types/eslint": "^8.4.1",
    "husky": "^4.3.7",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": ">= 3.0.0 < 5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
